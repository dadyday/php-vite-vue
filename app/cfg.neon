parameters:

tracy:
    dumpTheme: dark
    showBar: true

application:
	mapping:
		*: App\Presenters\*Presenter

latte:
	extensions:
		- App\ViteExtension(%wwwDir%, %VITE_PATH%, %VITE_HOST%)

database:
	dsn: 'sqlite:%rootDir%/data/demo'

security:
	authentication:
		# nach welcher Zeit der Inaktivit채t wird der Benutzer abgemeldet
		expiration: 30 minutes      # (string) Standardwert ist nicht gesetzt
		# wo Informationen 체ber den angemeldeten Benutzer gespeichert werden sollen
		storage: session            # (session|cookie) Voreinstellung ist session

	users:
		# Name: Passwort
		john: demo

		# Name, Passwort, Rolle und andere in der Identit채t verf체gbare Daten
		admin:
			password: demo123
			roles: [admin]
			data:

	roles:
		guest:
		user: [guest] # erbt von guest
		admin: [user] # erbt von user

	resources:
		foo:
		# bar: [foo]  # Ressource erbt von foo
		baz:

services:
	- App\RouterFactory::createRouter

	entities: App\Model\Entities

